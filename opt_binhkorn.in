# Dakota configuration file


environment
    graphics tabular_graphics_data

method
    moga
    output silent
    max_function_evaluations = 1000
    initialization_type unique_random
    crossover_type shuffle_random
    num_offspring = 2 num_parents = 2
    crossover_rate = 0.8
    mutation_type replace_uniform
    mutation_rate = 0.1
    fitness_type domination_count
    replacement_type below_limit = 6
    shrinkage_percentage = 0.9
    convergence_type metric_tracker
    percent_change = 0.05 num_generations = 40

model
  single

variables
  continuous_design = 2
    initial_point     1.0      1.0
    lower_bounds      0.0      0.0
    upper_bounds      5.0      3.0
    descriptors       'x1'     "x2"

interface
  analysis_drivers = "binhkorn"
    fork
    parameters_file = "x.in"
    results_file    = "y.out"

responses
  objective_functions = 2
  no_gradients
  no_hessians
